  "paths": {
    "/SaveRecipient": {
      "post": {
        "tags": [
          "Servisler"
        ],
        "summary": "Alıcı Kaydetme",
        "description": "İlk işlem olarak gönderinin alıcısını kaydetmek gerekmektedir. Bu işlem sonucunda dönülen __RecipientId__'sinin, gönderi kayıt aşamasıda __RecipientId__ olarak gönderilmesi beklenmektedir.",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/EmptyRequestBody"
              },
              "examples": {
                "CorporateRecipient": {
                  "summary": "Kurumsal Alıcı Kaydetme",
