{
    "name": "tcgunel/omniship-kolaygelsin",
    "description": "KolayGelsin (Sendeo) carrier for Omniship shipping library",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "tcgunel/omniship-common": "@dev"
    },
    "require-dev": {
        "pestphp/pest": "^3.0",
        "phpstan/phpstan": "^2.0",
        "guzzlehttp/guzzle": "^7.0",
        "guzzlehttp/psr7": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Omniship\\KolayGelsin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omniship\\KolayGelsin\\Tests\\": "tests/"
        },
        "files": [
            "tests/Helpers.php"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "../omniship",
            "options": {
                "symlink": true
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
